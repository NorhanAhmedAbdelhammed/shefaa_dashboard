{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build"
    },
    {
      "src": "package.json",
      "use": "vercel-sapper"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "dist"
}